<div id="content">
  <div class="shopify-section">
    <div class="">
      <div class="">
        <div class="">
          {{ section.settings.img_1 | image_url: width: 201 | image_tag }}
        </div>
        <div class="">
          {{ section.settings.img_2 | image_url: width: 201 | image_tag }}
        </div>
      </div>
      <div class="">
        <h2>
          {{ section.settings.heading }}
        </h2>
        <p>
          {{ section.settings.heading_desc }}
        </p>
      </div>
    </div>
  </div>
</div>

{% schema %}
  {
    "name": "Fifth Section",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading"
      },
      {
        "type": "text",
        "id": "heading_desc",
        "label": "Heading description"
      },
      {
        "type": "image_picker",
        "id": "img_1",
        "label": "Image 1"
      },
      {
        "type": "image_picker",
        "id": "img_2",
        "label": "Image 2"
      }, {
        "type": "color",
        "id": "background",
        "label": "Background"
      }
    ],
    "presets": [
      {
        "name": "Fifth Section",
        "category": "Text"
      }
    ]
  }
{% endschema %}
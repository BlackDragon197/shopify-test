    <div class="">
      <div class="">
        <div>
          {{ section.settings.heading }}
        </div>
        <div>
          {{ section.settings.thumb_1 | image_url: width: 60 | image_tag }}
        </div>
      </div>
      <div class="">
        <div>
          <div>
            <h5>{{ section.settings.label_1_head }}</h5>
          </div>
          <div>
            <span>{{ section.settings.label_1_desc }}</span>
          </div>
        </div>
        <div>
          <div>
            <h5>{{ section.settings.label_2_head }}</h5>
          </div>
          <div>
            <span>{{ section.settings.label_2_desc }}</span>
          </div>
        </div>
        <div>
          <div>
